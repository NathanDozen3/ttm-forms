{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ttm/form",
	"version": "1.0.0",
	"title": "TTM Form",
	"category": "widgets",
	"icon": "text",
	"description": "Add form.",
	"example": {},
	"supports": {
		"html": false,
		"multiple": true
	},
	"attributes": {
		"button__background_color": { 
			"type": "string",
			"default": "#86b7fe"
		},
		"button__background_color__active": { 
			"type": "string"
		},
		"body__color": { 
			"type": "string",
			"default": "#000000"
		},
		"body__background_color": { 
			"type": "string",
			"default": "#ffffff"
		}
    },
	"textdomain": "t3-faq",
	"editorScript": [ "file:./index.js", "t3-schema-view" ],
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
