{
	"name": "ttm-forms",
	"version": "1.0.0",
	"description": "Another form plugin",
	"author": "Twelve Three Media",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-copy-php",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-copy-php",
		"start:ttm-form": "wp-scripts start --webpack-src-dir=blocks/ttm-form/src --output-path=blocks/ttm-form/build",
		"start:ttm-input-date": "wp-scripts start --webpack-src-dir=blocks/ttm-input-date/src --output-path=blocks/ttm-input-date/build",
		"start:ttm-input-email": "wp-scripts start --webpack-src-dir=blocks/ttm-input-email/src --output-path=blocks/ttm-input-email/build",
		"start:ttm-input-hidden": "wp-scripts start --webpack-src-dir=blocks/ttm-input-hidden/src --output-path=blocks/ttm-input-hidden/build",
		"start:ttm-input-password": "wp-scripts start --webpack-src-dir=blocks/ttm-input-password/src --output-path=blocks/ttm-input-password/build",
		"start:ttm-input-tel": "wp-scripts start --webpack-src-dir=blocks/ttm-input-tel/src --output-path=blocks/ttm-input-tel/build",
		"start:ttm-input-text": "wp-scripts start --webpack-src-dir=blocks/ttm-input-text/src --output-path=blocks/ttm-input-text/build",
		"start:ttm-textarea": "wp-scripts start --webpack-src-dir=blocks/ttm-textarea/src --output-path=blocks/ttm-textarea/build"
	},
	"devDependencies": {
		"@wordpress/scripts": "^27.1.0"
	},
	"files": [
		"**/*"
	]
}
