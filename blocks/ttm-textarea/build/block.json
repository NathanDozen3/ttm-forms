    "label": {
      "type": "string",
      "default": ""
    },
    "placeholder": {
      "type": "string",
      "default": ""
    },
    "required": {
      "type": "boolean",
      "default": false
    },
    "sronly": {
      "type": "boolean",
      "default": false
    }
  },
  "parent": [
    "ttm/form"
  ],
  "textdomain": "ttm-form",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}