{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ttm/textarea",
  "version": "1.0.0",
  "title": "Text Area",
  "category": "widgets",
  "icon": "text",
  "description": "",
  "example": {},
  "supports": {
    "html": false,
    "multiple": true
  },
  "attributes": {
    "parentID": {
      "type": "string",
      "default": ""
